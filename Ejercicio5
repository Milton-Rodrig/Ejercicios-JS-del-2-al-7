	<h1>Ejercicios-5</h1>

<script type="text/javascript">
var numero1 = 5;
var numero2 = 8;

alert("si numero1 = 5 y numero2 = 8 " + " entonces: ");

if(numero1 <= numero2) {
  alert(" numero1 <= numero2 " + "\n" + "numero1 no es mayor que numero2");
}
if(numero2 >= 0) {
  alert("si numero2 >= 0 " + " \n " +  "numero2 es positivo");
}
if(numero1 < 0 || numero1 != 0) {
  alert("si numero1 < 0 || numero1 != 0 " + " \n " +  "numero1 es negativo o distinto de cero");
}
if(++numero1 < numero2) {
  alert(" ++numero1 < numero2 " + " \n " + "Incrementar en 1 unidad el valor de numero1 no lo hace mayor o igual que numero2");
}
</script>
</body>
